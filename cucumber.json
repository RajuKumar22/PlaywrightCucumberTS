{
    "default": {
        "paths": [
            "src/test/features/*.feature"
        ],
        "publishQuite": true,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json"
        ],
        "parallel": 2,
        "retry": 1
    }
}